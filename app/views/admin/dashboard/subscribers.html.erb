<div class='col-md-8 col-md-offset-2'>
  <table class="table">
    <tr>
      <th>Email</th>
      <th>Date</th>
      <th>Options</th>
    </tr>
    <% @subscribers.each do |subscriber| %>
      <tr>
        <td><%= subscriber.email %></td>
        <td><%= subscriber.created_at %></td>
        <td>
          <%= link_to "mailto:#{subscriber.email}" do %>
            <i class="fa fa-envelope fa-lg dark-icon"></i>
          <% end %>
          <%= link_to prelaunch_subscriber_path(subscriber), method: :delete do %>
            <i class="fa fa-ban fa-lg dark-icon"></i>
          <% end %>
        </td>
      </tr>
    <% end %>

  </table>
</div>
